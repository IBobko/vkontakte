<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/8" fx:controller="ru.todo100.social.vk.controllers.SettingsController"
      prefHeight="100" prefWidth="400" spacing="10">
    <HBox spacing="10">
        <children>
            <Label text="Ключ антикапчи">
                <opaqueInsets>
                    <Insets/>
                </opaqueInsets>
                <HBox.margin>
                    <Insets top="4.0"/>
                </HBox.margin>
            </Label>
            <TextField fx:id="antiCaptchaKeyField"/>

        </children>

    </HBox>
    <Button mnemonicParsing="false" onAction="#saveButtonAction" text="Сохраннить"></Button>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>